  "downtime": {
    "title": "Downtime Activities",
    "icon": "calendar",
    "activities": [
      {
        "name": "Communal Work",
        "icon": "heart-handshake",
        "description": "Spend time tending to the sick and the poor.",
        "benefit": "Gain favor with senior members, make local connections, and other benefits."
      },
      {
        "name": "Studying",
        "icon": "book-open",
        "description": "Attend classes in preparation for sitting an exam.",
        "benefit": "If you complete a full term's tuition, make your exam check with advantage."
      },
      {
        "name": "Research",
        "icon": "flask-conical",
        "requirement": "Speaker rank or above",
        "description": "Pursue research projects to improve existing medicines or develop new ones.",
        "benefit": "Same earnings as guild work, but no risk of complications.",
        "duration": "Takes workweeks equal to the new item's strength."
      }
    ],
    "guildWork": {
      "title": "Guild Work",
      "icon": "briefcase",
      "requirement": "Seeker rank or above",
      "intro": "Characters of seeker rank and above can spend downtime in the Crucible—a great alchemical workshop—filling orders placed by customers.",
      "resources": "Each day spent performing guild work counts as a full day of work. All necessary equipment and materials are supplied to you by the Guild.",
      "resolution": "The amount of money you earn performing guild work is based on your guild rank as follows:",
      "earnings": [
        { "rank": "Seeker", "earnings": "Modest lifestyle for the week + 5 gp per day of work" },
        { "rank": "Seer", "earnings": "Comfortable lifestyle for the week + 10 gp per day of work" },
        { "rank": "Speaker", "earnings": "Wealthy lifestyle for the week + 25 gp per day of work" },
        { "rank": "Master", "earnings": "Aristocratic lifestyle for the week + 50 gp per day of work" }
      ],
      "complicationsIntro": "Undertaking guild work rarely results in significant complications. Still, such activities aren't without risk. Each workweek spent filling orders brings a 10% chance a complication arises.",
      "complications": [
        { "roll": 1, "result": "A customer cancels an order, reducing your earnings for the week by one rank." },
        { "roll": 2, "result": "You secure a lucrative contract, increasing your earnings for the week by one rank." },
        { "roll": 3, "result": "Your tools are damaged or stolen, forcing you to buy new ones.*" },
        { "roll": 4, "result": "An incident occurs in the crucible for which you are blamed (justified or not, as you choose). You are barred from the crucible for 1d6 weeks.*" },
        { "roll": 5, "result": "A compound you are mixing explodes while you aren't wearing safety goggles. You are blinded for 24 hours as your sight slowly recovers." },
        { "roll": 6, "result": "Items have gone missing from stocks, and someone has pointed the finger at you. Until your name is cleared, the party is barred from using the guild merchant.*" }
      ],
      "complicationsNote": "* Might involve a rival."
    }
  },

